<h1>Tickets</h1>
<table>
  <thead>
    <tr>
      <th>Request Number</th>
      <th>Excavator Company</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.request_number %></td>
        <td><%= ticket.excavator.company_name %></td>
        <td><%= link_to 'Show', api_v1_ticket_path(ticket) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
